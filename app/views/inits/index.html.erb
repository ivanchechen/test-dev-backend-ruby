<p id="notice"><%= notice %></p>

<h1>Inits</h1>

<table>
  <thead>
    <tr>
      <th>Entrada</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inits.each do |init| %>
      <tr>
        <td><%= init.entrada %></td>
        <td><%= link_to 'Show', init %></td>
        <td><%= link_to 'Edit', edit_init_path(init) %></td>
        <td><%= link_to 'Destroy', init, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Init', new_init_path %>
